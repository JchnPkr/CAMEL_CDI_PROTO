<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_1.xsd"
        version="1.1">
  <session name="StartUpService">
    <resource-ref name="url/camel/loggerRef" binding-name="url/camel/logger"/>
    <resource-ref name="url/camel/propertiesRef" binding-name="url/camel/properties" />
    <resource-ref name="jdbc/dataSourceRef" binding-name="jdbc/camel/dataSource" />
  </session>
</ejb-jar-bnd>
